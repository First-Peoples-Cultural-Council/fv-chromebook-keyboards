{
  "name": "FirstVoices Keyboards",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "FirstVoices Keyboards for Chrome OS",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js",
      "keyboards/nuuc/main.js",
      "keyboards/smalgyax/main.js"
    ]
  },
  "options_page": "options.html",
  "permissions": [
    "input",
    "storage"
  ],
  "input_components": [
    {
      "name": "FirstVoices Keyboards",
      "type": "ime",
      "id": "fv-keyboards",
      "description": "FirstVoices Keyboards for Chrome OS",
      "language": "en-US",
      "layouts": ["us"]
    }
  ]
}
